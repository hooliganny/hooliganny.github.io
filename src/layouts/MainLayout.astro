---
import Sidebar from "../components/Sidebar.astro";
import Shell from "../layouts/Shell.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Shell title={title}>
  <body>
    <main class="flex flex-row">
      <Sidebar />
      <slot />
    </main>
  </body>
</Shell>
