---
import Shell from "./Shell.astro";

interface Props {
  title: string;
}

const { title } = Astro.props as Props;
---

<Shell title={title}>
  <body>
    <main
      class="w-screen h-screen flex flex-col items-center text-center animate-fade animate-duration-[3000ms] z-10"
    >
      <a class="absolute inset-0 w-full h-full" href="/projects"></a>

      <slot />
    </main>
  </body>
</Shell>
