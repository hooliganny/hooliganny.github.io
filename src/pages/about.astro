---
import MainLayout from "../layouts/MainLayout.astro";
import { getEntry } from "astro:content";
import MarkdownContent from "../components/MarkdownContent.astro";

const entry = await getEntry("about", "overview");
const { Content } = (await entry?.render()) ?? {};
---

<MainLayout
  title="About | hooliganny"
  heading="About"
  subHeading="Stuff and things relating to me, as well as the site"
>
  <MarkdownContent>
    <Content />
  </MarkdownContent>
</MainLayout>
