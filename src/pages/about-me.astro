---
import MainLayout from "../layouts/MainLayout.astro";
import { getEntry } from "astro:content";
import MarkdownContent from "../components/MarkdownContent.astro";
const entry = await getEntry("about", "overview");
const { Content } = (await entry?.render()) ?? {};
---

<MainLayout
  title="About me | hooliganny"
  heading="About me"
  subHeading="What I am willing to share about myself"
>
  <MarkdownContent>
    <Content />
  </MarkdownContent>
</MainLayout>
